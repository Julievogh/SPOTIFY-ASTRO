---
import RightNav from "../components/RightNav.astro";

import Playlists from "../components/Playlists.astro";

---

<div>
  <RightNav />

  <Playlists />
</div>

<style>
  div {
    background: rgb(18, 20, 20);
    background: linear-gradient(
      0deg,
      rgba(18, 20, 20, 1) 47%,
      rgba(50, 50, 54, 1) 100%
    );
    grid-column: 2/3;
    grid-row: 1/4;
    margin: 0.5rem;
    border-radius: 10px;
  }
</style>
